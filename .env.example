# 通义千问API配置
DASHSCOPE_API_KEY=your_dashscope_api_key_here

# 应用配置
APP_NAME=UCAP-Agent-Demo
APP_VERSION=1.0.0
DEBUG=true

# 数据库配置
DATABASE_PATH=./data/ucap_demo.db

# 日志配置
